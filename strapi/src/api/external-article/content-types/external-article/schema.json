{
  "kind": "collectionType",
  "collectionName": "external_articles",
  "info": {
    "singularName": "external-article",
    "pluralName": "external-articles",
    "displayName": "External Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "author": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "date": {
      "type": "date",
      "required": false
    },
    "link": {
      "type": "string",
      "required": true
    },
    "source": {
      "type": "string",
      "required": true
    },
    "source_url": {
      "type": "string",
      "required": true
    },
    "quote": {
      "type": "text"
    },
    "slug": {
      "type": "string",
      "unique": true
    },
    "newsletter_inclusion": {
      "type": "enumeration",
      "enum": [
        "none",
        "daily",
        "weekly",
        "both"
      ],
      "default": "none"
    },
    "searchTags": {
      "type": "string"
    },
    "searchableContent": {
      "type": "text",
      "private": true
    },
    "isSearchable": {
      "type": "boolean",
      "default": true
    },
    "searchPriority": {
      "type": "integer",
      "default": 5
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published",
        "archived"
      ],
      "default": "draft"
    },
    "metadata": {
      "type": "json",
      "private": true
    }
  }
}
