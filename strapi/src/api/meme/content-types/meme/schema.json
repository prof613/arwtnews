{
  "kind": "collectionType",
  "collectionName": "memes",
  "info": {
    "singularName": "meme",
    "pluralName": "memes",
    "displayName": "Meme",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "artist": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "string",
      "default": "Meme/Cartoons",
      "required": true
    },
    "date": {
      "type": "datetime"
    },
    "description": {
      "type": "text",
      "required": false
    },
    "artist_link_1": {
      "type": "string",
      "required": false
    },
    "artist_link_1_label": {
      "type": "enumeration",
      "enum": [
        "Website",
        "Facebook",
        "Instagram",
        "X",
        "Snapchat",
        "YouTube",
        "TikTok",
        "LinkedIn",
        "Portfolio",
        "Shop/Store",
        "Social Media"
      ],
      "required": false,
      "default": "Website"
    },
    "artist_link_2": {
      "type": "string",
      "required": false
    },
    "artist_link_2_label": {
      "type": "enumeration",
      "enum": [
        "Website",
        "Facebook",
        "Instagram",
        "X",
        "Snapchat",
        "YouTube",
        "TikTok",
        "LinkedIn",
        "Portfolio",
        "Shop/Store",
        "Social Media"
      ],
      "required": false,
      "default": "Social Media"
    },
    "image_path": {
      "type": "string"
    },
    "keep_image_in_assets": {
      "type": "boolean",
      "default": false
    },
    "newsletter_inclusion": {
      "type": "enumeration",
      "enum": [
        "none",
        "daily",
        "weekly",
        "both"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "artist",
      "required": true
    },
    "enable_share_buttons": {
      "type": "boolean",
      "default": true
    },
    "ogTitle": {
      "type": "string"
    },
    "ogImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "searchTags": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published",
        "archived"
      ],
      "default": "draft"
    }
  }
}
